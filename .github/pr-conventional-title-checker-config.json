{
  "LABEL": {
    "name": "no-conventional-pr-name"
  },
  "CHECKS": {
    "regexp": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\([\\w\\-\\.]+\\))?(\\!)?\\: ([\\w ])+([\\s\\S]*)",
    "ignoreLabels": ["WIP"]
  },
  "MESSAGES": {
    "success": "All OK",
    "failure": "Failing CI test",
    "notice": ""
  }
}
